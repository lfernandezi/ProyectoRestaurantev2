	<parameter name="NumeroPedido" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT bdrestaurantev2.pedido.codpedido,
	bdrestaurantev2.pedido.fechacreacion,
	bdrestaurantev2.pedido.monto,
	bdrestaurantev2.pedido.estadopedido,
	bdrestaurantev2.pedido.codcliente,
	bdrestaurantev2.detallepedido.codproducto,
	bdrestaurantev2.detallepedido.nombreproducto,
	bdrestaurantev2.detallepedido.cantidad,
	bdrestaurantev2.detallepedido.precio,
	bdrestaurantev2.detallepedido.subtotal,
	bdrestaurantev2.pedido.direccion,
	CONCAT (bdrestaurantev2.cliente.nombre,
	bdrestaurantev2.cliente.apellido) as cliente
FROM bdrestaurantev2.pedido
	inner join bdrestaurantev2.detallepedido ON 
	 bdrestaurantev2.pedido.codpedido = bdrestaurantev2.detallepedido.codpedido 
	inner join bdrestaurantev2.cliente ON 
	 bdrestaurantev2.cliente.codcliente = bdrestaurantev2.pedido.codcliente 
WHERE 
	 bdrestaurantev2.pedido.codpedido = $P{NumeroPedido}]]>
	</queryString>
